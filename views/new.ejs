<h1>Add a New Location</h1>

<form action="/location" method="POST">
  <input type="text" name="country" placeholder="country">
  <input type="text" name="country_key" placeholder="country_key">
  <input type="text" name="state" placeholder="state">
  <input type="text" name="state_key" placeholder="state_key">
  <input type="text" name="institution" placeholder="institution">
  <input type="text" name="institution_key" placeholder="institution_key">
  <input type="text" name="building" placeholder="building">
  <input type="text" name="building_key" placeholder="building_key">
  <input type="text" name="floor" placeholder="floor">
  <input type="text" name="floor_key" placeholder="floor_key">
  <input type="text" name="room" placeholder="room">
  <input type="text" name="room_key" placeholder="room_key">
  <button>Upload</button>
</form>
<a href="/">Go Back</a>